local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "Premium", HidePremium = true, IntroEnabled = false, SaveConfig = true, ConfigFolder = "BUNBASTER"})
local Tab = Window:MakeTab({
	Name = "Bios",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})
Tab:AddButton({
Name = "Back to Bios",
Callback = function()
loadstring(game:HttpGet(('https://raw.githubusercontent.com/vanya6583/GattaGatta/main/Main')))()
end    
})

local Tab = Window:MakeTab({
	Name = "Chat Commands",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})
Tab:AddToggle({
	Name = "This is a toggle!",
	Default = false,
	Callback = function(Value)
		_G.enabled = Value
if _G.enabled == true then
game.Players.LocalPlayer.Chatted:connect(function(msg)
if msg:sub(1, 6) == "@goto " then
    local player
    for i, plr in ipairs(game.Players:GetPlayers()) do
    if string.lower(plr.Name):sub(1, string.len(msg:sub(7))) == string.lower(msg:sub(7)) then
        player = plr.Name
    end
    end
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace[player].HumanoidRootPart.CFrame
end
end)
end
	end    
})


game.Players.LocalPlayer.Chatted:connect(function(msg)
if msg:sub(1, 6) == "@goto " then
    local player
    for i, plr in ipairs(game.Players:GetPlayers()) do
    if string.lower(plr.Name):sub(1, string.len(msg:sub(7))) == string.lower(msg:sub(7)) then
        player = plr.Name
    end
    end
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace[player].HumanoidRootPart.CFrame
end
end)
